@model IEnumerable<Dominio.Developer>

<h1>Developers</h1>
@if (Model.Count() == 0)
{
    <p>No existen developers</p>
}
else
{
    <div>
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Nombre y Apellido</th>                    
                    <th>Email</th>
                    <th>Lider</th>
                    <th>Activo</th>
                    <th></th>
                </tr>
            </thead>
            @foreach (Dominio.Developer d in Model)
            {
                <tbody>
                    <tr>
                        <td>@d.Name @d.LastName</td>
                        <td>@d.Email</td>
                        <td>@d.Leader.Name @d.Leader.LastName</td>
                        <td>@d.Active</td>
                        <td>
                            @Html.ActionLink("Editar", "EditDeveloper", "Home", new { id = d.Id })
                        </td>
                    </tr>
                </tbody>
            }
        </table>
    </div>
}
<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="CreateDeveloper">Crear developer</a>